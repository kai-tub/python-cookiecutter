{
    "plugins": [
        "git-tag",
        "all-contributors",
        "first-time-contributor",
        [
            "exec",
            {
                "beforeCommitChangelog": "bash .auto/update_version.sh"
            }
        ]
    ],
    "owner": "{{ cookiecutter.github_username }}",
    "repo": "{{ cookiecutter.project_slug }}",
    "name": "{{ cookiecutter.full_name }}",
    "email": "{{ cookiecutter.email }}"
}
